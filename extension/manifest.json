{
  "manifest_version": 3,
  "name": "PaperStore",
  "version": "2.0.0",
  "description": "Save arXiv papers and PDFs to your PaperStore library.",
  "permissions": ["activeTab", "tabs"],
  "host_permissions": [
    "https://papers.teleosis.ai/*",
    "https://arxiv.org/*",
    "https://*.arxiv.org/*",
    "https://alphaxiv.org/*",
    "https://*.alphaxiv.org/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Add to PaperStore"
  }
}
